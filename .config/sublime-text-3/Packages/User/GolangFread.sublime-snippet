package main

import "os"
import "fmt"
import "bufio"

func main() {
    file, _ := os.Open( os.Args[1] )
    scanner := bufio.NewScanner(file)
    defer file.Close()

    for scanner.Scan() {
        fmt.Printf( "%s\n", scanner.Text() )
    }

    os.Exit(0)
}
