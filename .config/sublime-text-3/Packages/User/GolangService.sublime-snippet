package main

import (
	"net/http"
)

func main() {
	http.HandleFunc("/", homepage)
	http.ListenAndServe(":8080", nil)
}

func homepage(w http.ResponseWriter, req *http.Request) {
	w.Write([]byte("Hello, World!"))
}
